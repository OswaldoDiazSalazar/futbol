{
    "parser": "babel-eslint",
    "rules": {
        "no-console": "warn",
        "react/prop-types": [0],
        "react/button-has-type": ["off", {
            "button": "off",
            "onClick": "off",
            "reset": "off"
          }]
    },
    "plugins": ["react", "import", "jsx-a11y"],
    "extends": ["airbnb"],
    "env": {
        "browser": true,
        "node": true,
    }
}